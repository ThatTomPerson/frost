    "preprod": "npm run libphonenumber-metadata",
    "prod": "webpack --env.NODE_ENV=production --env.production --progress",
    "build": "npm start -- -d",
    "watch": "webpack --watch --progress",
    "serve": "nodemon --exec \"npm run hot-server -- --inline\" --watch webpack.config.babel.js",
    "serve-status": "nodemon --exec \"npm run hot-server\" --watch webpack.config.babel.js",
    "hot-server": "webpack-dev-server --progress --hot --hot-only --open",
    "start": "webpack --progress",
    "preprofile": "mkdir -p public/build && webpack -p --profile --progress --json > public/build/stats.json",
    "profile": "webpack-bundle-analyzer public/build/stats.json",
    "force-quit": "ps aux | grep webpack | grep -v grep | awk '{print $2}' | xargs -n1 kill -9",
